//For finding minimum path between each pair of node in n*n cities.
class Solution {
  public:
    void floydWarshall(vector<vector<int>> &mat) {
        int n = mat.size();
        for(int k=0;k<n;k++) {
            for(int i=0;i<n;i++) {
                for(int j=0;j<n;j++) {
                    if(mat[i][k]==1e8 or mat[k][j]==1e8) continue;
                    mat[i][j] = min(mat[i][j],mat[i][k]+mat[k][j]);
                }
            }
        }
    }
};
